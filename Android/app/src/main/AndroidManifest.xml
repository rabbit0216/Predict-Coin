<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.predictcoin">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/predict_coin_icon_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/predict_coin_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PredictCoin"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Milk_Coin"
            android:exported="false" />
        <activity
            android:name=".Ripple_Coin"
            android:exported="false" />
        <activity
            android:name=".Bora_Coin"
            android:exported="false" />
        <activity
            android:name=".Sandbox_Coin"
            android:exported="false" />
        <activity
            android:name=".Solana_Coin"
            android:exported="false" />
        <activity
            android:name=".Ada_Coin"
            android:exported="false" />
        <activity
            android:name=".Ethereum_Coin"
            android:exported="false" />
        <activity
            android:name=".BTC_Coin"
            android:exported="true" />
        <activity
            android:name=".Activity_Fraud"
            android:exported="true" />
        <activity
            android:name=".Activity_Notification"
            android:exported="true" />
        <activity
            android:name=".LoadingActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>